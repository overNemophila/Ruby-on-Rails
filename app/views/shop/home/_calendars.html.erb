<%= form_with class: "row", method: :get do |form| %>
  <div class="col-auto">
    表示範囲
  </div>
  <div class="col-auto">
    <%= form.date_field(:start_at, value: @start_at.strftime("%F"), class: "form-control") %>
  </div>
  <div class="col-auto">
    <%= form.date_field(:end_at, value: @end_at.strftime("%F"), class: "form-control") %>
  </div>
  <div class="col-auto">
    <%= form.submit("検索", class: "btn btn-primary") %>
  </div>
<% end %>